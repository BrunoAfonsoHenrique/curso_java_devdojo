package io.test;

import java.io.File;
import java.io.IOException;

public class FileTest {

	public static void main(String[] args) {
		
		File arquivo = new File("C:\\Users\\Usuário\\Desktop\\devdojo\\io\\arquivoscriados\\Arquivo.txt");
		try {
			boolean novoArquivo = arquivo.createNewFile();
			System.out.println(novoArquivo);
			System.out.println(arquivo.exists());
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
