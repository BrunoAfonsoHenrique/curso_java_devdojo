package io.test;

import java.io.Console;

public class Consoletest {

	public static void main(String[] args) {
		
		//Console --> Classe que trabalha com entrada de dados via terminal
		
		Console c = System.console();
		char[] pw = c.readPassword("%s", "pw: ");
		for(char pass: pw) {
			c.format("%c", pass);
		}
		c.format("\n");
		

	}

}
